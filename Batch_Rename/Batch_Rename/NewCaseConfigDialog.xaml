<Window x:Class="Batch_Rename.NewCaseConfigDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Batch_Rename"
        mc:Ignorable="d"
        Title="NewCaseConfigDialog" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <TextBlock Text="   Edit New Case" Width="103" Margin="356,139,333,245" HorizontalAlignment="Center"/>
        <TextBlock Text="   Type Case:" Width="79" HorizontalAlignment="Center" Margin="262,200,451,190"/>
        <ComboBox Name="newcaseComboBox" Height="20" Width="100">
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding TypeNewCase}"/>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>
        <Button Name="newcasebtnOk" Content="OK" Margin="262,276,441,110" Click="NewcasebtnOk_Click"/>
        <Button Name="newcasebtnCancel" Content="Cancel" Margin="372,276,333,110" Click="NewcasebtnCancel_Click"/>
    </Grid>
</Window>
