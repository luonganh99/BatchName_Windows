<Window x:Class="Batch_Rename.ReplaceConfigDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ReplaceConfigDialog" Height="276.534" Width="325.6">
    <Canvas>
        <Image Width="30" Height="30" Source="/imgs/config.png" Canvas.Left="78" Canvas.Top="21"></Image>
        <Label Canvas.Left="114" Canvas.Top="19" FontFamily="Segoe UI Semibold" FontSize="16" Content="REPLACE CONFIG" FontWeight="Bold" />
        <Label Canvas.Left="29" Canvas.Top="76" FontFamily="Segoe UI Semibold" FontSize="14">From</Label>
        <Label Canvas.Left="29" Canvas.Top="124" FontFamily="Segoe UI Semibold" FontSize="14">To</Label>
        <TextBox Name="fromTextBox"  Height="30" Canvas.Left="89" Canvas.Top="76" Width="183" FontFamily="Segoe UI Semibold" FontSize="14"/>
        <TextBox Name="toTextBox"  Height="30" Canvas.Left="89" Canvas.Top="124" Width="183" FontFamily="Segoe UI Semibold" FontSize="14"/>
        <Button Name="btnOk" Width="60" Height="30" Click="btnOk_Click" Canvas.Left="162" Canvas.Top="201" FontFamily="Segoe UI Semibold" FontSize="14">OK</Button>
        <Button Width="60" Height="30" IsCancel="True" Canvas.Left="239" Canvas.Top="201" FontFamily="Segoe UI Semibold" FontSize="14">Cancel</Button>
    </Canvas>
</Window>
